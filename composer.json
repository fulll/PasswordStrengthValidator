{
    "name": "fulll/password-strength-validator",
    "description": "Password-strength validator for Symfony",
    "keywords": ["password", "validator", "symfony"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollercapes.net"
        },
        {
            "name": "Luis Cordova",
            "email": "cordoval@gmail.com"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/rollerworks/PasswordStrengthValidator/contributors"
        }
    ],
    "require": {
        "php": "^5.5 || ^7.0",
        "psr/container": "^1.0",
        "symfony/polyfill-mbstring": "^1.5.0",
        "symfony/validator": "^2.8.9 || ^3.3.6 || ^4.0"
    },
    "require-dev": {
        "symfony/config": "^3.3.6 || ^4.0",
        "symfony/console": "^3.3.6 || ^4.0",
        "symfony/phpunit-bridge": "^3.3.6 || ^4.0"
    },
    "autoload": {
        "psr-4": {
            "Rollerworks\\Component\\PasswordStrength\\": "src/"
        },
        "exclude-from-classmap": ["test/"]
    },
    "autoload-dev": {
        "psr-4": {
            "Rollerworks\\Component\\PasswordStrength\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
